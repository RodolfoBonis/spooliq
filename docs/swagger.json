            "post": {
                "security": [
                    {
                        "Bearer": []
                    }
                ],
                "description": "Create a new filament brand",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Brands"
